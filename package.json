{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@3tatsu/passport-discord",
  "version": "1.0.4",
  "description": "Passport strategy for authentication with Discord (discord.com)",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@9.7.0",
  "readme": "README.md",
  "type": "commonjs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3tatsu/passport-discord.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "passport",
    "discord",
    "auth",
    "authentication",
    "identity",
    "types",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "start": "ts-node-dev --respawn --transpile-only src/index.ts",
    "prepare": "husky"
  },
  "author": {
    "name": "3tatsu",
    "url": "https://github.com/3tatsu"
  },
  "dependencies": {
    "passport-oauth2": "^1.8.0",
    "tslib": "^2.6.3"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.15",
    "@types/passport-oauth2": "^1.4.17",
    "discord-api-types": "^0.37.94",
    "eslint": "^8.57.0",
    "eslint-config-neon": "^0.1.62",
    "husky": "^9.1.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
